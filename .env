# 5 bots (example)
BOT_TOKENS=111:AAA,222:BBB,333:CCC,444:DDD,555:EEE

# Use one or many emojis (comma-separated)
EMOJIS=🔥,❤️,😂,👏,👍

# Modes: cycle | random | same
EMOJI_MODE=cycle

# Big animated reaction (true/false)
IS_BIG=false

# Optional: stagger requests per bot (ms)
PER_BOT_DELAY_MS=120

# Optional: HTTP client timeout (seconds)
HTTP_TIMEOUT=15
